            <TextView
                android:id="@+id/tvTeamMemberName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ivTeamMemberPic_kislaya"
                android:text="Kislaya Mishra"
                android:textColor="#07040c"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvTeamMemberEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvTeamMemberName"
                android:layout_toRightOf="@id/ivTeamMemberPic_kislaya"
                android:text="kislaya_mishra@students.iitmandi.ac.in"
                android:textSize="12dp"/>
        </LinearLayout>

        <ImageView
